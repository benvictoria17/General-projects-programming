   "source": [
    "A = [3, 1, 6, 7, 2, 9] #initialize a list\n",
    "\n",
    "for i in range(1, len(A)): #algorithm\n",
    "    for j in reversed(range(i)): \n",
    "        if (A[j+1] < A[j]):\n",
    "            A[j+1], A[j] = A[j], A[j+1]\n",
    "        else:\n",
    "            break\n",
    "\n",
    "print(A)"
   ]
